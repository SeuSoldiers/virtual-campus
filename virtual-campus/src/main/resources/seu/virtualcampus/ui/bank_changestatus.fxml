<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seu.virtualcampus.ui.bank_changestatusController">
   <children>
      <Label layoutX="116.0" layoutY="100.0" text="当前用户状态：">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="statustext" layoutX="232.0" layoutY="90.0" prefHeight="39.0" prefWidth="208.0" style="-fx-background-color: #e0f7e4;">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="116.0" layoutY="155.0" text="修改用户状态：">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button fx:id="lostbtn" layoutX="281.0" layoutY="174.0" mnemonicParsing="false" onAction="#changestatus_lost" prefHeight="39.0" prefWidth="98.0" text="申请挂失" textFill="#987f1a">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Button>
      <Button fx:id="nolostbtn" layoutX="281.0" layoutY="237.0" mnemonicParsing="false" onAction="#changestatus_nolost" prefHeight="39.0" prefWidth="98.0" text="取消挂失" textFill="#2a961b">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Button>
      <Button fx:id="closebtn" layoutX="281.0" layoutY="302.0" mnemonicParsing="false" onAction="#changestatus_close" prefHeight="39.0" prefWidth="98.0" text="销户" textFill="#c92115">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Button>
      <Button fx:id="backbtn" layoutX="26.0" layoutY="21.0" mnemonicParsing="false" onAction="#changestatus_back" prefHeight="31.0" prefWidth="66.0" text="返回">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
