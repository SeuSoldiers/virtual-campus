<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0"
            prefWidth="600.0"
            style="-fx-background-color: linear-gradient(to bottom, #a1c4fd, #c2e9fb);" xmlns="http://javafx.com/javafx/24.0.1"
            fx:controller="seu.virtualcampus.ui.bank.bank_depositController">
    <children>
        <Label layoutX="81.0" layoutY="100.0" prefHeight="36.0" prefWidth="146.0" text="请输入存入金额："
               textFill="#071a98">
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
        </Label>
        <TextField fx:id="amounttext" layoutX="143.0" layoutY="148.0" prefHeight="57.0" prefWidth="314.0">
            <font>
                <Font size="18.0"/>
            </font>
        </TextField>
        <Button fx:id="nobtn" layoutX="141.0" layoutY="270.0" mnemonicParsing="false" onAction="#deposit_no"
                prefHeight="36.0" prefWidth="107.0" text="取消存款" textFill="#ba3838">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Button>
        <Button fx:id="yesbtn" layoutX="352.0" layoutY="270.0" mnemonicParsing="false" onAction="#deposit_yes"
                prefHeight="36.0" prefWidth="107.0" text="确定存款" textFill="#1fa625">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Button>
    </children>
</AnchorPane>
