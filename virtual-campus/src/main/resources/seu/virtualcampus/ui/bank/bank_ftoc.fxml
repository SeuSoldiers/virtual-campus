<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="503.0"
            prefWidth="799.0"
            style="-fx-background-color: linear-gradient(to bottom, #a1c4fd, #ffffff);" xmlns="http://javafx.com/javafx/24.0.1"
            fx:controller="seu.virtualcampus.ui.bank.bank_ftocController">
    <children>
        <TableView fx:id="ftocTableView" layoutX="25.0" layoutY="100.0" prefHeight="295.0" prefWidth="750.0"
                   style="-fx-border-width: 6px; -fx-border-color: #5a93f4;">
            <columns>
                <TableColumn fx:id="idColumn" editable="false" prefWidth="127.33334350585938" text="交易记录ID"/>
                <TableColumn fx:id="amountColumn" editable="false" prefWidth="78.6666259765625" text="金额"/>
                <TableColumn fx:id="yearColumn" editable="false" prefWidth="144.666748046875" text="定期年限"/>
                <TableColumn fx:id="timeColumn" editable="false" prefWidth="142.6666259765625" text="交易时间"/>
                <TableColumn fx:id="rateColumn" editable="false" prefWidth="76.0" text="利率"/>
                <TableColumn fx:id="interestColumn" editable="false" prefWidth="79.33331298828125" text="利息"/>
                <TableColumn fx:id="statusColumn" editable="false" prefWidth="93.3333740234375" text="状态"/>
            </columns>
        </TableView>
        <Button fx:id="backbtn" layoutX="19.0" layoutY="21.0" mnemonicParsing="false" onAction="#ftoc_back"
                prefHeight="29.0" prefWidth="57.0" text="返回">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Button>
        <Button fx:id="refreshbtn" layoutX="94.0" layoutY="21.0" mnemonicParsing="false" onAction="#ftoc_refresh"
                prefHeight="29.0" prefWidth="57.0" text="刷新" textFill="#3e8f35">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Button>
        <Label layoutX="151.0" layoutY="431.0" prefHeight="19.0" prefWidth="152.0" text="请输入你的密码：">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Label>
        <Label layoutX="256.0" layoutY="50.0" style="-fx-background-color: #afb6fc; -fx-background-radius: 10px;"
               text="请选择你的定期存款记录：" textFill="#1616a3">
            <font>
                <Font name="System Bold" size="24.0"/>
            </font>
        </Label>
        <Button fx:id="yesbtn" layoutX="508.0" layoutY="419.0" mnemonicParsing="false" onAction="#ftoc_yes"
                prefHeight="43.0" prefWidth="137.0"
                style="-fx-background-color: linear-gradient(to bottom, #a1c4fd, #c2e9fb);" text="确认定期转活期"
                textFill="#2745db">
            <font>
                <Font name="System Bold" size="14.0"/>
            </font>
        </Button>
        <PasswordField fx:id="passwordtext" layoutX="261.0" layoutY="426.0" prefHeight="29.0" prefWidth="198.0"/>
    </children>
</AnchorPane>
